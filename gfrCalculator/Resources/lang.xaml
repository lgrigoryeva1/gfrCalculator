<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:gfrCalculator.Resources"
                    xmlns:v="clr-namespace:System;assembly=mscorlib">
    <v:String x:Key="m_Title">GFR calculator</v:String>
    <v:String x:Key="m_Header">Calculate the Glomerular Filtration Rate (GFR) for Children under 18</v:String>
    <v:String x:Key="m_Height">Height: </v:String>
    <v:String x:Key="m_cmUnit">cm</v:String>
    <v:String x:Key="m_Serum">Serum creatinine: </v:String>
    <v:String x:Key="m_SerumUnitOne">mg/dL</v:String>
    <v:String x:Key="m_SerumUnitTwo">µmol/L</v:String>
    <v:String x:Key="m_Calculate">Calculate</v:String>
    <v:String x:Key="m_Reset">Reset</v:String>
    <v:String x:Key="m_GFR">GFR is</v:String>
    <v:String x:Key="m_Result">mL/min/1.73m³</v:String>
    <v:String x:Key="m_Language">Language</v:String>
</ResourceDictionary>